<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - CountDown</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.devexpress.com/jslib/21.2.6/css/dx.common.css">
    <link rel="stylesheet" href="https://cdn.devexpress.com/jslib/21.2.6/css/dx.light.css">

    @Styles.Render("/Content/style.css")
    @*@Styles.Render("/Content/Bootstrap/bootstrap.css")*@
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <navbar class="mt-5">
        <h1 class="text-center">CountDown Tool</h1>
    </navbar>
    @RenderBody()

    <footer>
    </footer>

    <script src="https://cdn.devexpress.com/jslib/21.2.6/js/dx.all.js"></script>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/Content/script.js")
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery-countdown@2.2.0/dist/jquery.countdown.min.js"></script>
    <script src="https://cdn.rawgit.com/hilios/jQuery.countdown/2.2.0/dist/jquery.countdown.min.js"></script>
    <script src="~/Content/util.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
